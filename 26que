//26. Finding the biggest out of n integers
#include<stdio.h>
int main()
{
	int n,i,c,big;
	printf("enter how many elements");
	scanf("%d",&n);
	printf("enter a element %d",n);
	printf("element ");
	scanf("%d",&big);
	for(i=2;i<=n;i++)
	{
	printf("enter element",i);
	scanf("%d",c);
	if(big<c)
	big=c;	
	}
	printf("largest number is %d",n,big);
	return 0;
	
}
