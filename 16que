16. Summing up any n numbers and finding average
#include<stdio.h>
int main()
{
	int i,n,sum=0,num;
	float avg;
	printf("enter how many  value");
	scanf("%d",&n);
	printf("enter the values");
	for(i=0;i<n;i++)
	{
		scanf("%d",&num);
		sum=sum+num;
	}
	avg=sum/n;
	printf("sum of numbers %d",sum);
	printf("avg of numbers %f",avg);
	return 0;
}
